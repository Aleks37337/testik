<!--Foto-gallerya->
<script>
    //инициализируем галерею ДО запуска слайдера
    let gallery = $('.main-slide a');
    //при клике на ссылку в слайде запускаем галерею
    $('.main-slide a').on('click', function(e) {
        e.preventDefault();
        //узнаём индекс слайда без учёта клонов
        let totalSlides = +$(this).parents('.main-slider').slick("getSlick").slideCount,
            dataIndex = +$(this).parents('.main-slide').data('slick-index'),
            trueIndex;
        switch(true){
            case (dataIndex<0):
                trueIndex = totalSlides+dataIndex; break;
            case (dataIndex>=totalSlides):
                trueIndex = dataIndex%totalSlides; break;
            default:
                trueIndex = dataIndex;
        }
        //вызывается элемент галереи, соответствующий индексу слайда
        $.fancybox.open(gallery,{}, trueIndex);
        return false;
    });
    $('.main-slider').slick({
        slidesToShow: 3,
        dots: true,
        appendArrows: $('.your-class-arrow'),
        prevArrow: '<button id="prev" type="button" class="slick-arrow slick-prev"><</button>',
        nextArrow: '<button id="next" type="button" class="slick-arrow slick-next">></button>',
        customPaging: function() {
            return ''
        },
        responsive: [
            {
                breakpoint: 768,
                settings: {
                    slidesToShow: 2
                }
            },
            {
                breakpoint: 480,
                settings: {
                    slidesToShow: 1
                }
            }
        ]
    });
</script>
<!-/Foto-gallerya-->
<!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++Start Accordeon++++++++++-->
<script>
    let acc = document.getElementsByClassName("accordion");
    let i;
    for (i = 0; i < acc.length; i++) {
        acc[i].addEventListener("click", function() {
            this.classList.toggle("active");
            let panel = this.nextElementSibling;
            if (panel.style.display === "block") {
                panel.style.display = "none";
            } else {
                panel.style.display = "block";
            }
        });
    }
</script>
<!--+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++/Accordeon++++++++++-->
