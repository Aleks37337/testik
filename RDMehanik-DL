<script>
    $(".nav_link_mob").on("click", function() {
        //            $(".open_menu_link_mob").css("display", "none");
        $(".open_menu_link_mob").css("transform", "translate3d(100%, 0px, 0px) scale3d(1, 1, 1) rotateX(0deg) rotateY(0deg) rotateZ(0deg) skew(0deg, 0deg)");
        $(".open_menu_link_mob").css("transform-style", "preserve-3d");
    });
</script>
<!--/Menu-->
<!--Slowly enter-->
<script>
    $(".nav_link, .nav_link_mob").on("click", function(e) {
        e.preventDefault();
        let anchor = $(this).attr('href');
        $('html, body').stop().animate({
            scrollTop: $(anchor).offset().top - 30
        }, 1200);
    });
</script>
