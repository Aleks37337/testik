<!--?ScrollTop-->
<script>
    $("a.up").on("click", function(e) {
        e.preventDefault();
        let anchor = $(this).attr('href');
        $('html, body').stop().animate({
            scrollTop: $(anchor).offset().top - 60
        }, 800);
    });
</script>
<!--/?ScrollTop-->
<!--ScrollTop-->
<script>
    let btn = $('a.up');
    $(window).scroll(function() {
        if ($(window).scrollTop() > 100) {
            btn.css('opacity', '0.5');
        } else {
            btn.css('opacity', '0');
        }
    });
</script>
<!--/ScrollTop-->
